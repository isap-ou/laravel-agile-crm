{
  "name": "isapp/laravel-agile-crm",
  "description": "Laravel HTTP Client for AgileCRM",
  "keywords": [
    "framework",
    "laravel",
    "laravel-package",
    "agile-crm",
    "php"
  ],
  "type": "library",
  "require": {
    "illuminate/http": "^10 | ^11"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^11.0.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Isapp\\AgileCrm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Isapp\\AgileCrm\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "ISAP OÃœ",
      "email": "contact@isap.me",
      "homepage": "https://isap.me"
    },
    {
      "name": "Andrii Trush",
      "email": "andrii.trush@isapp.be",
      "homepage": "https://isapp.be"
    }
  ],
  "support": {
    "email": "contact@isap.me"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Isapp\\AgileCrm\\ServiceProvider"
      ],
      "aliases": {
        "AgileCrm": "Isapp\\AgileCrm\\Facades\\AgileCrm"
      }
    }
  }
}
